    "### Realizamos LASSO para predecir las masas:"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 78,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "X1 = transpose([log10(R_cota),log10(Vperpendicular_cota),log10(abs((Vparalela_cota)))])\n",
    "y1 = log10(MasaT_Cota[:,0])\n",
    "\n",
    "n1 = X1.shape[0]\n",
    "X1_train, y1_train = X1[:int(n1 *0.25)], y1[:int(n1 *0.25)]\n",
    "X1_test, y1_test = X1[int(n1 *0.25):], y1[int(n1 *0.25):]\n",
    "xlim\n",
    "\n",
    "alpha = 1E-14\n",
    "lasso1 = Lasso(alpha=alpha)\n",
    "\n",
    "y1_pred_lasso = lasso1.fit(X1_train, y1_train).predict(X1_test)\n",
    "\n",
    "\n",
    "r2_score1_lasso = r2_score(y1_test, y1_pred_lasso)"
   ]
