   ],
   "source": [
    "X2 = transpose([log10(R_cota),log10(abs(Vparalela_cota)),log10(MasaT_Cota[:,0])])\n",
    "y2 = log10(Vperpendicular_cota)\n",
    "\n",
    "n2 = X2.shape[0]\n",
    "X2_train, y2_train = X2[:int(n2 *0.25)], y2[:int(n2 *0.25)]\n",
    "X2_test, y2_test = X2[int(n2 *0.25):], y2[int(n2 *0.25):]\n",
    "\n",
    "alpha = 1E-17\n",
    "lasso2 = Lasso(alpha=alpha)\n",
    "\n",
    "y2_pred_lasso = lasso2.fit(X2_train, y2_train).predict(X2_test)\n",
    "\n",
    "coef2 =lasso2.coef_.reshape(3, 1)\n",
    "coef2"
   ]
